        <%var pg_prev = parseInt(locals.pg) - 1 %>
        <% link = "/search/?q="+ locals.q+ "&page=" + pg_prev  %>
        <a href="<%=link%>" class="prev-pg-btn link-enabled"><i class="fas fa-arrow-left"></i> prev
        </a>
        <%}%>
        <% if(locals.pg == locals.total_pg){%>
            <a href="" class="next-pg-btn link-disabled">next<i class="fas fa-arrow-right"></i>
            </a>
        <%} else {%>
            <%var pg_next = parseInt(locals.pg) + 1 %>
            <% link = "/search/?q="+ locals.q+ "&page="+ pg_next %>
            <a href="<%=link%>" class="next-pg-btn link-enabled">next<i class="fas fa-arrow-right"></i>
            </a>
