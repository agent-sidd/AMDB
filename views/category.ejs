              <h2 style="text-align: center;"><%=locals.q%></h2>
              <p style="text-align: center;">Try with different one </p>
          </div>
            <%} else {%>
        <div class="box-conatiner row ">
            <% locals.data.forEach(function(mv){%>
                <%if(mv.media_type == 'movie'){%>
                    <% var link = '/details/mv?id=' + mv.id %>
                    <%} else {%>
                <% var link = '/details/tv?id=' + mv.id %>
                <%}%>
        <div class="card-wrapper clm-lg-2 clm-md-3 clm-sm-4 clm-6 mb-3">
        <a href="<%=link%> " class="movie-item movieItem ">
            <%if(mv.poster_path == null){%>
                    
                <%var img ="/images/noimagesmall.jpg" %>
                <% } else{ %>
            <% var img = 'https://www.themoviedb.org/t/p/w220_and_h330_face/'+ mv.poster_path%>
            <%}%>

            <img src="<%=img%>" alt="">
            <div class="movie-item-content">

                <div class="movie-item-title">
                    <%=mv.original_title%>
                    <%=mv.original_name%>
                </div>
                <div class="movie-infos">
                    <div class="movie-info">
                        <i class="bx bxs-star"></i>
                        <span><%=mv.vote_average%> </span>
                    </div>
                    <div class="movie-info">
                        <i class="bx bxs-captions"></i>
                        <span> <%=mv.original_language %></span>
                    </div>
                    <div class="movie-info">
                        <span>HD</span>
                    </div>
                    <div class="movie-info">
                        <span>16+</span>
                    </div>
                </div>
            </div>
            <div class="card-cat">
                <%if(mv.media_type == 'movie'){%>
                <span class="mv">MV</span>
                <%} else {%>
                    <span class="tv"> TV</span>
                    <%}%>
            </div>
        </a>
        
    </div>  
    <%})%>
